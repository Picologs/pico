# Fly.io ignore file - files and directories to exclude from deployment

# Node modules (installed during build)
node_modules/

# Development and test files
.env.local
.env.test
.env.development.local
test/
*.test.ts
*.spec.ts

# Documentation
*.md
!README.md

# IDE
.vscode/
.idea/
*.swp
*.swo

# Git
.git/
.gitignore
.github/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS files
.DS_Store
Thumbs.db

# Scripts (testing only, not needed in production)
scripts/

# Misc
.npmrc
secrets/
*.pem

# Railway and other platform configs
railway.toml
.railway/
RAILWAY_SETUP.md
FLY_SETUP.md

# Development databases
picologs/
Game.log

# Build artifacts (if any)
dist/
build/
target/
